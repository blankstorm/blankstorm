precision highp float;
#include "utils.glslx"

varying vec2 vUV;

uniform float face;
uniform sampler2D sampler;
uniform float resolution;
uniform float seed;

export void cloudFragmentShader() {
	float color = scalar_field(int(face), vUV, seed, resolution, sampler);
	gl_FragColor = vec4(vec3(color), 1.0);
}