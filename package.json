{
	"name": "blankstorm-client",
	"version": "1.0.0-prtototype",
	"description": "Blankstorm",
	"main": "core.js",
	"scripts": {
		"format:check": "npx prettier --check *.{html,js}",
		"format:fix": "npx prettier --write *.{html,js}",
		"format": "npm run format:check && npm run format:fix",
		"lint:check-old": "npx eslint *.js",
		"lint:fix": "npx eslint  --fix *.js",
		"lint:check": "npx eslint -f node_modules/eslint-formatter-azure-devops/index.js *.js",
		"lint": "npm run lint:check && npm run lint:fix",
		"test": "npm run format:check && npm run lint:check"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dr-vortex/blankstorm.git"
	},
	"keywords": [
		"blankstorm",
		"dr-vortex",
		"gui"
	],
	"author": "Dr. Vortex",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/dr-vortex/blankstorm/issues"
	},
	"homepage": "https://github.com/dr-vortex/blankstorm#readme",
	"devDependencies": {
		"eslint": "^8.24.0",
		"eslint-formatter-azure-devops": "^0.2.0",
		"prettier": "^2.7.1"
	}
}
